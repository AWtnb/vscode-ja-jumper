{
  "name": "ja-jumper",
  "displayName": "ja-jumper",
  "description": "Cursor-jumper for Japanese (and other any language) punctuations.",
  "icon": "images/logo.png",
  "repository": {
    "url": "https://github.com/AWtnb/vscode-ja-jumper"
  },
  "publisher": "awtnb",
  "version": "0.9.2",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Cursor",
    "keybindings"
  ],
  "activationEvents": [
    "onCommand:ja-jumper.jumpFore",
    "onCommand:ja-jumper.jumpBack",
    "onCommand:ja-jumper.jumpForeSelect",
    "onCommand:ja-jumper.jumpBackSelect",
    "onCommand:ja-jumper.swapAnchor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "ja-jumper",
      "properties": {
        "ja-jumper.delimiters": {
          "type": "string",
          "default": "、，。．；：「」『』【】（）〔〕《》〈〉［］“”‘’・！？～／…―　←↓↑→○●▲△▼▽◆◇■□★☆〓[]().,=<>:;`'\" #/-",
          "description": "delimiters for cursor jump."
        },
        "ja-jumper.isGreedy": {
          "type": "boolean",
          "default": true,
          "description": "if true, jump as far as possible."
        }
      }
    },
    "keybindings": [
      {
        "command": "ja-jumper.jumpFore",
        "key": "alt+j",
        "when": "editorTextFocus"
      },
      {
        "command": "ja-jumper.jumpBack",
        "key": "alt+k",
        "when": "editorTextFocus"
      },
      {
        "command": "ja-jumper.jumpForeSelect",
        "key": "alt+shift+j",
        "when": "editorTextFocus"
      },
      {
        "command": "ja-jumper.jumpBackSelect",
        "key": "alt+shift+k",
        "when": "editorTextFocus"
      },
      {
        "command": "ja-jumper.swapAnchor",
        "key": "alt+shift+s",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
